{
    "name": "@elizaos/plugin-dao",
    "version": "1.0.0",
    "description": "Plugin for managing DAO state and interactions",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --watch"
    },
    "dependencies": {
        "@elizaos/core": "workspace:*"
    },
    "devDependencies": {
        "tsup": "^6.5.0",
        "typescript": "^4.9.4"
    }
}
